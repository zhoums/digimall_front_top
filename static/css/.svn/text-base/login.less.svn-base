@borderColor: #e6e6e6;
@subFontColor: #555;
h2
{
    font-size: 24px;

    margin-bottom: 20px;
}
.boxPage
{
    padding: 30px 0 0;
}
.boxDetail
{
    margin: 0 0 15px;
    li
    {
        margin-bottom: 12px;
    }
    li label
    {
        font-size: 14px;

        display: inline-block;

        width: 150px;
        padding-right: 10px;

        text-align: right;

        color: @subFontColor;
    }
    li a
    {
        font-size: 14px;

        margin-left: 10px;

        color: #0069ef;
    }
    li input
    {
        width: 300px;
        padding: 10px 15px;

        border: 1px solid #e5e5e5;
    }
    .region
    {
        width: 332px;
        padding: 9px 15px;

        border: 1px solid #e5e5e5;
    }
}
.boxSave
{
    margin-left: 162px;
    li
    {
        margin-bottom: 10px;
    }
    li input
    {
        vertical-align: middle;
        // margin-right:3px;
    }
    .registerBtn,
    .loginBtn
    {
        width: 330px;
    }
}
.btnPrimary
{
    font-size: 14px;
    font-weight: bold;

    width: 160px;
}
.error_msg
{
    padding-left: 162px;

    color: red;
}
.infoLeft
{
    float: left;
}
.infoRight
{
    float: right;
}
.loginWrapper
{
    overflow: hidden;
    .boxSave
    {
        li
        {
            position: relative;
        }
        li a
        {
            font-size: 14px;
        }
        .toRegisterBtn
        {
            position: absolute;
            right: 5px;

            text-decoration: underline;
        }
    }
}
.registerWrapper
{
    .infoRight
    {
        font-size: 14px;

        padding: 15px 20px;

        border: 1px solid @borderColor;
        .toLoginBtn
        {
            line-height: 26px;

            width: 200px;
            margin-top: 10px;
        }
    }
}


